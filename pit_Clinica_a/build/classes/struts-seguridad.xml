<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org./dtds/struts-2.3.dtd">
<struts>
	<package name="struts-seguridad" extends="demo" namespace="/seguridad">
		<action name="loginUsuario" class="action.UsuarioAction" method="login">
			<result name="input" type="tiles">d_principal</result>
			<result name="login">/WEB-INF/paginas/login.jsp</result>
			
			
		</action>
		
		<action name="logoutUsuario" class="action.UsuarioAction" method="logout">
			<result name="login" type="redirectAction">
				<param name="actionName">inicio</param>
				<param name="namespace">/</param>
			</result>
			
			
		</action>
	</package>
</struts>